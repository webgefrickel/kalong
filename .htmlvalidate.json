{
  "extends": [
    "html-validate:recommended"
  ],
  "rules": {
    "no-trailing-whitespace": "off",
    "svg-focusable": "off",
    "doctype-style": [
      "error",
      {
        "style": "lovercase"
      }
    ],
    "void-style": [
      "error",
      {
        "style": "selfclosing"
      }
    ]
  }
}
