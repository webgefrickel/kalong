{
  "extends": [
    "html-validate:recommended"
  ],
  "rules": {
    "no-trailing-whitespace": "off",
    "svg-focusable": "off",
    "void-style": [
      "error",
      {
        "style": "selfclosing"
      }
    ],
    "no-autoplay": [
      "error",
      {
        "include": [
          "audio"
        ],
        "exclude": [
          "video"
        ]
      }
    ]
  }
}
